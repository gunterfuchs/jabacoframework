]]></Source><Param Name="(Name)" Value="VBMenu"/><Param Name="(SuperClass)" Value="javax/swing/JMenuItem"/><Param Name="(Type)" Value="Class"/><Param Name="(Access)" Value="Public"/></SourceFile>
